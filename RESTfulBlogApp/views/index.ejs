<% include ./partials/header %>

<h1>index page</h1>

<% blogs.forEach(function(blog){ %>
<div>
  <h2><%=blog.title%></h2>
  <img src="<%= blog.image%>" width="200" height="121" >
  <span><%= blog.created %></span>
  <p><%= blog.body%></p>
  <a class="ui floated basic violet button" href="/blogs/<%= blog._id %>">
   Read More
 </div>
 <% }) %>

 <% include ./partials/footer %>
